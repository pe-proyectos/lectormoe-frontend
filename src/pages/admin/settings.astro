---
import AdminLayout from '../../layouts/AdminLayout.astro';
import { AdminSettings } from '../../components/admin/AdminSettings.jsx';

const { logged, organization } = Astro.locals;

if (!logged) {
  Astro.redirect("/login");
}
---

<AdminLayout title="Opciones" page="settings">
    <AdminSettings organization={organization} client:only />
</AdminLayout>
